{
  "enabledPlugins": {
    "frontend-design@claude-plugins-official": true,
    "rust-analyzer-lsp@claude-plugins-official": true
  },
  "autoUpdatesChannel": "latest",
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(cd:*)",
      "Bash(gh pr list:*)",
      "Bash(xargs git log:*)",
      "Bash(git checkout:*)",
      "Bash(git pull:*)",
      "Bash(git stash:*)",
      "Bash(git add:*)",
      "Bash(git diff:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr edit:*)",
      "Bash(gh api:*)",
      "Bash(uv run python:*)",
      "Bash(uv sync:*)",
      "Bash(uv add:*)",
      "Bash(uv run:*)",
      "Bash(uv pip list:*)",
      "Bash(docker:*)",
      "Bash(devcontainer build:*)",
      "Bash(head:*)",
      "Bash(devcontainer read-configuration:*)",
      "Bash(devcontainer up:*)",
      "Bash(tail:*)",
      "Bash(echo:*)",
      "Bash(cat:*)"
    ]
  }
}
